        div(style="display: none;position: relative; z-index: 2")#hiddenWrapp
        .app-dialog-content
            a(href="/").close-modal
            .modal
                .modal__left
                    .modal__title посчитать <br> заказ

                    .text-cursive 
                        | Просчитайте заказ у нас 
                        br 
                        span.pink-title
                            | и 
                            .elemment-pink получите скидку!

                    .modal__text Ваша заказ будет обработан нашими менеджерами. В ближайшие минуты мы свяжемся с вами.

                    .wrap-modal__calc--desc
                        //- .modal__order Ваш заказ
                        //- .modal__order-item Стоимость готовой продукции: 
                        //-     span.modal__order-item.modal__order-item_bold 0 грн.
                        //- .modal__order-item Цена за один экземпляр: 
                        //-     span.modal__order-item.modal__order-item_bold 0 грн./екз
                        //- .modal__order-item Вес заказа:
                        //-     span.modal__order-item.modal__order-item_bold 0 кг
                        button#submit-main-form.button-order.__long
                            span отправить заказ

                .modal__right.form
                    form( action="http://test.artprintstudio.com.ua/lang/masseg/zayvka/" method="POST" name="test")#main-form-modal
                        .form__title ваши данные
                        div#form-input-wrap1
                            input#form-input1.form__input(type="name", name="name", placeholder="Ваше имя")
                        div#form-input-wrap2
                            input#form-input2.form__input(type="number", name="tell", placeholder="Телефон")
                        div#form-input-wrap3
                            input#form-input3.form__input(type="email", name="email", placeholder="E-mail")
                        div#form-input-wrap4
                            textarea#form-input4.form__input(type="text", name="comment", placeholder="Комментарий")

